// primeiro passo do prisma: Reconhecer e estruturar o banco de dados
// segundo passo : Operar sobre o banco de dados reconhecido e estruturado 

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}


 model datasOfConsults {
   id String @id @default(uuid())
   nome String
   cnpj String
   numeroDeTelefone String
   cidade String
   estado String
   distrito String
   nomeDaRua String
   numeroDaRua String
   cep String
   @@map("dados_das_consultas")
 } 
